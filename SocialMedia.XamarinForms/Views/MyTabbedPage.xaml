<?xml version="1.0" encoding="utf-8" ?>
<rxui:ReactiveTabbedPage
    x:Class="SocialMedia.XamarinForms.Views.MyTabbedPage"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:SocialMedia.XamarinForms.Views.Tabs"
    xmlns:rxui="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms"
    xmlns:vm="clr-namespace:SocialMedia.XamarinForms.ViewModels"
    x:TypeArguments="vm:MyTabbedViewModel">
    <local:HomeTab
        x:Name="firstTab"
        Title="Home"
        Visual="Material" />
    <local:MessagesTab
        x:Name="secondTab"
        Title="Messages"
        Visual="Material" />
    <local:NotificationsTab
        x:Name="thirdTab"
        Title="Notifications"
        Visual="Material" />
    <local:ProfileTab
        x:Name="fourthTab"
        Title="Profile"
        Visual="Material" />
</rxui:ReactiveTabbedPage>
